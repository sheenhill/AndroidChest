<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android">
    <objectAnimator
        android:duration="@integer/animator_duration"
        android:propertyName="translationX"
        android:valueFrom="@dimen/u_dp_85"
        android:valueTo="@dimen/dp_0" />
    <objectAnimator
        android:duration="@integer/animator_duration"
        android:propertyName="scaleY"
        android:valueFrom="0.92"
        android:valueTo="1" />
    <!--旋转-->
    <objectAnimator
        android:duration="@integer/animator_duration"
        android:propertyName="rotationY"
        android:valueFrom="@integer/u_angle_30"
        android:valueTo="@integer/angle_0" />
</set>